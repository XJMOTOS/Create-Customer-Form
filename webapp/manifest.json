{
  "_version": "1.65.0",
  "sap.app": {
    "id": "beamsuntoryinc.createcustomerform",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.4",
      "toolsId": "6f268d6f-c0cc-4a10-bf82-4f8d6aba951b"
    },
    "dataSources": {
      "GWD_C_FORM_UI5": {
        "uri": "/sap/opu/odata/sap/ZGSP_CUSTOMER_CFORM_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      "GWD_BPA_UI5": {
        "uri": "/sap/opu/odata/sap/ZGSP_CUST_CREATE_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "rootView": {
			"viewName": "beamsuntoryinc.createcustomerform.view.App",
			"type": "XML",
			"id": "app"
		},
    "dependencies": {
      "minUI5Version": "1.138.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
				"sap.uxap": {},
				"sap.suite.ui.microchart": {}
      }
    },
    "handleValidation": true,
    
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "beamsuntoryinc.createcustomerform.i18n.i18n"
        }
      },
      "GWD_C_FORM_UI5": {
        "dataSource": "GWD_C_FORM_UI5",
        "preload": true,
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Inline",
					"useBatch": true
				}
			},
      "GWD_BPA_UI5": {
        "dataSource": "GWD_BPA_UI5",
        "preload": true,
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Inline",
					"useBatch": true
				}
			}
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"controlId": "app",
				"controlAggregation": "pages"
			},
			"routes": [
				{
					"pattern": "",
					"name": "main",
					"target": "main"
				}
			],
			"targets": {
				"main": {
					"viewId": "main",
					"name": "beamsuntoryinc.createcustomerform.view.Main",
					"type": "View"
				}
			}
		}
  }
}
